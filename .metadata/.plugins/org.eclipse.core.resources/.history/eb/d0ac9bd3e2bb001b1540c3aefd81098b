create database uait;
use uait;

create table ADDRESS(
Addressid INT(255) PRIMARY KEY,
Street varchar(50) NOT NULL,
Town varchar(50) NOT NULL,
District varchar(50) NOT NULL,
State varchar(50) NOT NULL,
zipcode varchar(50) NOT NULL,
aadharNumber varchar(50),
epicId varchar(50) ,

);

alter table address add unique(aadharnumber);
alter table address add unique(epicId);

#FOREIGN KEY(aadharNumber) REFERENCES AADHARCard(aadharNumber) ON DELETE SET NULL ON UPDATE CASCADE,
#FOREIGN KEY(epicId) REFERENCES VoterCard(epicId) ON DELETE SET NULL ON UPDATE CASCADE,


insert into ADDRESS (addressid,street,town,district,state,zipcode) values
