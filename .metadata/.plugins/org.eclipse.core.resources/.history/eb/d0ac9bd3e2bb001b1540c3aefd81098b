("182637123922","Prateek Bansal","1995-10-09","male",1111225633),
("111637281922","Vishal Tulsani","1997-12-11","male",1111222433);


create table VOTERCARD(
epicId varchar(50) PRIMARY KEY,
name varchar(50) NOT NULL,
DOB date NOT NULL,
gender varchar(15) NOT NULL,
addressID INT(255)NOT NULL,
FOREIGN KEY(addressID) REFERENCES ADDRESS(addressID)
);


