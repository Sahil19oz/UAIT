create table AADHARCARD(
aadharnumber varchar(50) PRIMARY KEY,
name varchar(50) NOT NULL,
DOB date NOT NULL,
gender varchar(15) NOT NULL,
addressID INT(255)NOT NULL,
FOREIGN KEY(addressID) REFERENCES ADDRESS(addressID)
);

insert into AADHARCARD values
("182637281922","Sahil Agarwal","1997-08-19","male",1111225633),
("182633331922","Harsh Agarwal","1994-03-10","male",1111200233),
